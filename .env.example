# Route Configuration
# Copy this file to .env and fill in your values

# Start location
START_LAT=...
START_LON=...
START_NAME=Home
START_ADDRESS=City, State

# End location
END_LAT=...
END_LON=...
END_NAME=Destination
END_ADDRESS=City, State

# Segment Configuration (optional)
# Mode: 'distance' (default) or 'count'
# SEGMENT_MODE=distance

# Miles per segment (when mode is 'distance')
# MILES_PER_SEGMENT=1

# Number of segments (when mode is 'count')
# NUM_SEGMENTS=10
